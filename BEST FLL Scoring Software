import turtle

wn = turtle.Screen()
wn.title('FLL Tournament Scores')
wn.bgcolor('white')
wn.setup(width=800, height=600)
wn.tracer(0)



pen = turtle.Turtle()
pen.speed(0)
pen.color("black")
pen.penup()
pen.hideturtle()
pen.goto(0, 365)
pen.write("FLL Tournament Scores", align="center", font=("Arial", 24, "bold"))


pen.goto(-375, 300)
pen.write("Team Rank", align="center", font=("Arial", 18, "bold"))

pen.goto(-535, 300)
pen.write("Score (Total)", align="center", font=("Arial", 18, "underline", 'bold'))

